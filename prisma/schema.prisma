generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url = env("DATABASE_URL")
  relationMode = "prisma"
}

model Guestbook{
  id String @id @map("_id") @default(auto()) @db.ObjectId
  message String
  username String
  created_at DateTime @default(now())
}
